# {{ ansible_managed }}
---
apiVersion: v1
kind: Service
metadata:
  name: external-vault
  namespace: {{ specification.kubernetes_namespace }}
spec:
  ports:
  - protocol: TCP
    port: 8200
---
apiVersion: v1
kind: Endpoints
metadata:
  name: external-vault
  namespace: {{ specification.kubernetes_namespace }}
subsets:
  - addresses:
      - ip: {{ ansible_default_ipv4.address }}
    ports:
      - port: 8200
